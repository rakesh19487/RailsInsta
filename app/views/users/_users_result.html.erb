<% if users.present? %>
    <% users.each do |user| %>
        <%= link_to user_path(user), class: "no-text-decoration normal-color", local: true do %>
            <div class="search-result-item">
                <%= user.username %>
            </div>    
        <% end %>
    <% end%>
<% else %>
    <div class="p-3">
        No results found
    </div>
<% end %>